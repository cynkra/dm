{
  "tools": {
    "bash": {
      "allowedCommands": [
        "devcontainer up --workspace-folder .",
        "devcontainer exec --workspace-folder . *",
        "devcontainer build --workspace-folder .",
        "devcontainer --version",
        "touch *"
      ],
      "requireConfirmation": false
    }
  },
  "hooks": {
    "bash": {
      "pre": "devcontainer exec --workspace-folder . bash -c"
    }
  }
}
